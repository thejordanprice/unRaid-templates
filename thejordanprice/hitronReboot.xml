<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/thejordanprice/unRaid-templates/master/thejordanprice/hitronReboot.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Networking</Category>
  <Name>hitronreboot</Name>
  <Description>A cronjob container for reboot a modem on a regular schedule at 3:45am.</Description>
  <Overview>I will soon add a way to change the time for reboot.</Overview>
  <Support>#</Support>
  <Registry>https://hub.docker.com/r/thejordanprice/hitronreboot</Registry>
  <GitHub>https://github.com/thejordanprice/docker-hitronReboot</GitHub>
  <Repository>thejordanprice/hitronreboot</Repository>
  <Project>https://github.com/thejordanprice/hitronReboot</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Environment>
    <Variable>
      <Name>MODEM_ADDY</Name>
      <Value>192.168.100.1</Value>
    </Variable>
    <Variable>
      <Name>MODEM_USER</Name>
      <Value>cusadmin</Value>
    </Variable>
    <Variable>
      <Name>MODEM_PASS</Name>
      <Value>changeme</Value>
    </Variable>
  </Environment>
  <Data/>
  <Icon>https://i.imgur.com/0VCnltU.png</Icon>
</Containers>